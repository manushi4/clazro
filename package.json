{
  "name": "ManushiCoaching",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:dev": "react-native run-android --active-arch-only --mode=devDebug",
    "android:fast": "react-native run-android --mode=devDebug --active-arch-only",
    "android:prod": "react-native run-android --mode=prodDebug",
    "android:arm64": "react-native run-android --active-arch-only -- -PreactNativeArchitectures=arm64-v8a",
    "android:clean": "cd android && gradlew clean && cd ..",
    "android:clean-cache": "cd android && gradlew cleanBuildCache && cd ..",
    "android:clean-app": "cd android && gradlew :app:clean && cd ..",
    "android:install": "adb install android/app/build/outputs/apk/dev/debug/app-dev-x86_64-debug.apk",
    "android:build": "cd android && gradlew assembleDevDebug --parallel --build-cache && cd ..",
    "android:build-fast": "cd android && gradlew assembleDevDebug --parallel --build-cache -x lint -x test && cd ..",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm run typecheck && npm run lint",
    "validate:fix": "npm run typecheck && npm run lint:fix",
    "prebuild": "npm run validate",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle"
  },
